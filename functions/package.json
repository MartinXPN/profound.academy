{
  "name": "functions",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "serve": "yarn build && firebase emulators:start --only functions",
    "emulate": "(yarn build -- --watch) | (firebase emulators:start --import=./test_data --export-on-exit)",
    "shell": "yarn build && firebase functions:shell",
    "start": "yarn shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "pretest": "tsc -p tsconfig.dev.json",
    "test": "nyc mocha './test/**/*.ts' --reporter spec --require ts-node/register --recursive"
  },
  "engines": {
    "node": "16"
  },
  "eslintIgnore": [
    "*.spec.ts",
    "world.js",
    "test"
  ],
  "main": "lib/index.js",
  "dependencies": {
    "aws-sdk": "^2.1072.0",
    "cors": "^2.8.5",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.18.0",
    "moment": "^2.29.1",
    "needle": "^3.0.0",
    "notion-client": "^4.16.1"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^2.0.2",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.0",
    "@types/needle": "^2.5.3",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^0.3.3",
    "mocha": "^9.2.0",
    "nyc": "^15.1.0",
    "sinon": "^13.0.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
