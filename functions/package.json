    "firebase-functions": "^3.14.1",
    "needle": "^2.9.0",
    "notion-client": "^4.8.2"
  },
  "devDependencies": {
    "@types/needle": "^2.5.2",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-functions-test": "^0.3.2",
    "typescript": "^3.8.0"
  },
